{
  "scripts": [
    {
      "run": "after",
      "snippet": "COPY config_data (jsonb) FROM 'data/locales.data' ENCODING 'UTF8';",
      "fromModuleVersion": 0
    }
  ],
  "tables": [
    {
      "tableName": "config_data",
      "withMetadata": true,
      "withAuditing": true,
      "generateId": true,
      "populateJsonWithId": true,
      "ginIndex": [
        {
          "fieldName": "value",
          "tOps": "ADD",
          "caseSensitive": false,
          "removeAccents": true
        } 
      ],
      "likeIndex": [
        {
          "fieldName": "configName",
          "tOps": "ADD"
        },
        {
          "fieldName": "code",
          "tOps": "ADD"
        }
      ],
      "index": [
        {
          "fieldName": "module",
          "tOps": "ADD"
        }
      ]
    }
  ]
}
